async def ninth_response(update, context):
    global all_answers, right_answers
    reply_keyboard = [['–ò—Å–ø–∞–Ω–∏—è', '–ò—Ç–∞–ª–∏—è'],
                      ['–§—Ä–∞–Ω—Ü–∏—è', '–ì–µ—Ä–º–∞–Ω–∏—è']]
